{
  "name": "emoji-util",
  "version": "1.0.0",
  "description": "A simple emoji detection library",
  "main": "dist/emoji-util.js",
  "scripts": {
    "build": "rollup -c",
    "build:min": "cross-env NODE_ENV=production rollup -c rollup.config.production.js",
    "precompile": "del \"dist/**/*\"",
    "compile": "npm run build & npm run build:min",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register"
  },
  "keywords": [
    "emoji",
    "javascript",
    "es6"
  ],
  "author": "Dustin Schau <dustinschau@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/DSchau/emoji-util"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "~6.14.0",
    "babel-plugin-external-helpers": "~6.8.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-es2015-rollup": "~1.2.0",
    "chai": "~3.5.0",
    "cross-env": "~2.0.1",
    "del-cli": "~0.2.0",
    "mocha": "~3.0.2",
    "rollup": "~0.35.11",
    "rollup-plugin-babel": "~2.6.1",
    "rollup-plugin-uglify": "~1.0.1"
  }
}
